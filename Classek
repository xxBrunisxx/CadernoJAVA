fun main(args: Array<String>) {
  class Carro{
  var marca = ""
  var modelo = ""
  var ano = 0
} 
  // Criar um objeto (instanciar)
val fuca = Carro()

// Access the properties and add some values to it
fuca.marca = "Wolks"
fuca.modelo = "Fusca"
fuca.ano = 1975

println(fuca.marca)   // Mostra Wolks
println(fuca.modelo)   // Mostra Fusca
println(fuca.ano)    // Mostra 1975

  //intanciar outro carro e importar e imprimir os atributos

  var Cadillac = Carro()

  Cadillac.marca =  "Cadillac"
  Cadillac.modelo = "Cadillac s√©ries 62"
  Cadillac.ano = 1962

  println(Cadillac.marca)   // Mostra Cadillac
  println(Cadillac.modelo)   // Mostra Cadillac
  println(Cadillac.ano)    // Mostra 1962

}
