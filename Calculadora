fun main() {
  println("Digite um numero:")
  var num1 =  readLine()!!
   var x = num1.toInt()
  println("O numero é: " +num1)
  println("Digite outro numero:")
  var num2  =  readLine()!!
   var y = num2.toInt()
  println("O numero é: " +num2)
  var soma=x+y
  println("O resultado da soma é: " +soma)
  var sub=x-y
  println("O resultado da subtraçao é: " +sub)
  var mult=x*y
  println("O resultado da multiplicaçao é: " +mult)
  var div=x/y
  println("O resultado divisao é: " +div)
  
}
